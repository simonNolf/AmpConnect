<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.youtubeapikotlin.ui.home.HomeFragment"
        android:label="YTB"
        tools:layout="@layout/fragment_Toutube" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.youtubeapikotlin.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.youtubeapikotlin.ui.notifications.NotificationsFragment"
        android:label="Music Local"
        tools:layout="@layout/fragment_local" />
</navigation>